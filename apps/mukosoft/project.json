{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/mukosoft/src",
  "prefix": "mukosoft",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/mukosoft",
        "index": "apps/mukosoft/src/index.html",
        "main": "apps/mukosoft/src/main.ts",
        "polyfills": "apps/mukosoft/src/polyfills.ts",
        "tsConfig": "apps/mukosoft/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/mukosoft/src/favicon.ico",
          "apps/mukosoft/src/assets",
          {
            "glob": "**/*",
            "input": "src/assets",
            "output": "assets"
          }
        ],
        "styles": [
          "apps/mukosoft/src/theme/variables.scss",
          "apps/mukosoft/src/global.scss",
          "apps/mukosoft/src/assets/icon/flaticons/css/uicons-regular-rounded.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "2mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/mukosoft/src/environments/environment.ts",
              "with": "apps/mukosoft/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "mukosoft:build:production"
        },
        "development": {
          "browserTarget": "mukosoft:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "proxyConfig": "apps/mukosoft/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "mukosoft:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/mukosoft/**/*.ts",
          "apps/mukosoft/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/mukosoft"
      ],
      "options": {
        "jestConfig": "apps/mukosoft/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
